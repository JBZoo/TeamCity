{
    "name"              : "jbzoo/teamcity",
    "type"              : "library",
    "description"       : "Tools for JetBrains TeamCity CI",
    "license"           : "MIT",
    "keywords"          : [
        "TeamCity", "CI", "Tools", "Reports", "Clover", "pdepend", "PHP Depend", "phploc", "phpmetrics"
    ],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"           : ">=5.4",
        "jbzoo/console" : "^1.1",
        "jbzoo/utils"   : "^1.6.1"
    },
    "require-dev"       : {
        "jbzoo/phpunit"   : "^1.5.1",
        "symfony/process" : "^2.8|^3.0"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\TeamCity\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true,
        "bin-dir"             : "bin"
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/TeamCity/issues"
    }
}
